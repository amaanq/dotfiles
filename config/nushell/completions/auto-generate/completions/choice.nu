# Specify the list of choices to be created
extern "choice /c" [

	...args
]

# Hide the list of choices
extern "choice /n" [

	...args
]

# Specify that the choices are case-sensitive
extern "choice /cs" [

	...args
]

# Specify the number of seconds
extern "choice /t" [

	...args
]

# Specify the default choice
extern "choice /d" [

	...args
]

# Specify a message
extern "choice /m" [

	...args
]

# Show help
extern "choice /?" [

	...args
]